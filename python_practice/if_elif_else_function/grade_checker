def get_grade(score: int) -> str:
    return "Test"

print(get_grade(95))  # Should print "Test"

def get_grade(score: int) -> str:
    if score >= 90:
        return "A"
    elif score >= 80:
        return "B"
    elif score >= 70:
        return "C"
    elif score >= 60:
        return "D"
    else:
        return "F"

print(get_grade(95))
print(get_grade(72))
print(get_grade(50))


