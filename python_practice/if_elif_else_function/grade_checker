#Write a function that takes a numeric grade (0â€“100) and returns a letter grade based on the following scale:

def get_grade(score: int) -> str:
    if score >= 90:
        return "A"
    elif score >= 80:
        return "B"
    elif score >= 70:
        return "C"
    elif score >= 60:
        return "D"
    else:
        return "F"
